

lexer section:

    # define 'name' testing LPR first to test order independence

    LPR0            : LPR3 LPR3 LPR3 ;

    LPR1            : begin 'abc' end ;
    LPR2            : any any any ;
    LPR3            : 'a' [bd] 'c' ;

parser section:

    # define 'name' testing PPR first to test order independence

    PPR0            : PPR3 PPR4 PPR3 ;

    PPR1            : begin 'abc' end ;
    PPR2            : any any any ;
    PPR3            : token ;
    PPR4            : failure ;

