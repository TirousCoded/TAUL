

# rules outside explicit lexer/parser section are within
# an implicit lexer section

    LPR0        : any ;

lexer section:

    LPR1	    : any ;
    skip LPR2	: any ;

parser section:

    PPR0		: any ;

    # syntactic, but semantically illegal

    skip PPR1   : any ;

lexer section:

    LPR3		: any ;

parser section:

    PPR2        : any ;

